# ===== LiteLLM Updater Configuration =====

# HTTP port the updater will listen on
PORT=8000

# Number of worker processes uvicorn should spawn
UVICORN_WORKERS=1

# Uvicorn log level (debug, info, warning, error, critical)
LOG_LEVEL=info

# Seconds between image update checks for watchtower
WATCHTOWER_POLL_INTERVAL=60


# ===== LiteLLM Server Configuration =====

# Store LiteLLM models in database (required for local LiteLLM deployment)
STORE_MODEL_IN_DB=True

# LiteLLM master key for API authentication
LITELLM_MASTER_KEY=sk-1234

# LiteLLM salt key for encryption
LITELLM_SALT_KEY=sk-1234

# LiteLLM web UI credentials
UI_USERNAME=admin
UI_PASSWORD=admin


# ===== PostgreSQL Database Configuration =====

# PostgreSQL database name for LiteLLM
POSTGRES_DB=litellm

# PostgreSQL user for LiteLLM
POSTGRES_USER=llmproxy

# PostgreSQL password for LiteLLM
POSTGRES_PASSWORD=dbpassword9090

# Full PostgreSQL connection URL for LiteLLM
DATABASE_URL=postgresql://llmproxy:dbpassword9090@db:5432/litellm
